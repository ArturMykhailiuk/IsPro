#13;&#10;       Tara:='';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo39OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo39, Engine do&#13;&#10;  begin&#13;&#10;       Text:=sumStr(allSum);&#13;&#10;  end&#13;&#10;end;&#13;&#10;function sumStr(sum:real):string;&#13;&#10;begin&#13;&#10;//ДЛЯ ЦЕЛОЙ ЧАСТИ СУММЫ&#13;&#10;     SvlTxt.Close;&#13;&#10;     allNumInt:=Trunc(sum);&#13;&#10;     strAllNumInt:=copy(IntToStr(allNumInt),length(IntToStr(allNumInt)) , 1); //последняя цифра целой части числа в строковом представлении&#13;&#10;     case strAllNumInt of&#13;&#10;          '1': ValRod:=1;&#13;&#10;          '2','3','4': ValRod:=2&#13;&#10;          else ValRod:=3;&#13;&#10;          end;&#13;&#10;     SvlTxt.ParamByName('ValRcd').Value:=&#60;main.&#34;VALRCD&#34;&#62;;&#13;&#10;     SvlTxt.ParamByName('ValRod').Value:=ValRod;&#13;&#10;     SvlTxt.Open;&#13;&#10;     intStr:=BPRO.NumToTxt(allNumInt,ValRod,1);&#13;&#10;     if allNumInt = 0 then intStr:='Ноль';&#13;&#10;     intVal:=&#60;SVlTxt.&#34;SVlTxt_NmO&#34;&#62;;&#13;&#10;     SvlTxt.Close;&#13;&#10;//ДЛЯ ДРОБНОЙ ЧАСТИ СУММЫ&#13;&#10;     allNumFrac:= Int(round(Frac(sum)*100)); //дробная часть числа в числовом представлении&#13;&#10;     strAllNumFrac:=copy(IntToStr(allNumFrac),length(IntToStr(allNumFrac)) , 1);//последняя цифра дробной части числа в строковом представлении&#13;&#10;     case strAllNumFrac of&#13;&#10;          '1': ValRod:=1;&#13;&#10;          '2','3','4': ValRod:=2&#13;&#10;          else ValRod:=3;&#13;&#10;          end;&#13;&#10;     SvlTxt.ParamByName('ValRcd').Value:=&#60;main.&#34;VALRCD&#34;&#62;;&#13;&#10;     SvlTxt.ParamByName('ValRod').Value:=ValRod;&#13;&#10;     SvlTxt.Open;&#13;&#10;     if allNumFrac&#60;10 then&#13;&#10;        case allNumFrac of&#13;&#10;             0: strAllNumFrac:='00'&#13;&#10;             else strAllNumFrac:= '0' + intToStr(allNumFrac)&#13;&#10;             end&#13;&#10;     else strAllNumFrac:=IntToStr(allNumFrac);&#13;&#10;     fracVal:=&#60;SVlTxt.&#34;SVlTxt_NmR&#34;&#62;;&#13;&#10;     result:=intStr + ' ' + &#60;main.&#34;SVL_cd&#34;&#62; + ' '+varToStr(intVal)+ ' ' + strAllNumFrac + ' ' + varToStr(fracVal) + ' коп.';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo22OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     case i of&#13;&#10;          1 : strName:='наименование';&#13;&#10;          2,3,4: strName:='наименования'&#13;&#10;          else strName:='наименований';&#13;&#10;          end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo44OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     AdrFirm := ' ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_IND&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_IND&#34;&#62; + ', ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_CNTNM&#34;&#62;)) &#60;&#62; 0) then            //---05.05.2018-ВВ-Михайлюк А.Ю.---//, додано 'м.Київ', по причині відсутності значення поля в&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_CNTNM&#34;&#62; + ', м.Київ, '; //   таблиці(Картка підприємства)&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_REGNM&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_REGNM&#34;&#62; + ', ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_RAINM&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_RAINM&#34;&#62; + ', ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_NASNM&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_NASNM&#34;&#62; + ', ';&#13;&#10;&#13;&#10;    // if (Length(Trim(&#60;main.&#34;CRTADR_TOWNM&#34;&#62;)) &#60;&#62; 0) then          //---05.05.2018-ВВ-Михайлюк А.Ю.---//, закоментовано по причині відсутності значення поля в&#13;&#10;    //    AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_TOWNM&#34;&#62; + ', ';       //   таблиці(Картка підприємства)&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_STRNM&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_STRNM&#34;&#62; + ', ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_HOUSE&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_HOUSE&#34;&#62; + ', ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_NUMKOR&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_NUMKOR&#34;&#62; + ', ';&#13;&#10;&#13;&#10;     if (Length(Trim(&#60;main.&#34;CRTADR_NUMKV&#34;&#62;)) &#60;&#62; 0) then&#13;&#10;        AdrFirm := AdrFirm + &#60;main.&#34;CRTADR_NUMKV&#34;&#62;;&#13;&#10;&#13;&#10;     delete(AdrFirm, Length(AdrFirm)-1, 1);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo57OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//---10.05.18---ВВ-Михайлюк А.Ю.-Згідно завдання Замовника---//----------------------------------------------------------------------------&#13;&#10;   //---алгоритм пошуку документ-парента для накладної&#13;&#10;   osnova := '';&#13;&#10;   if &#60;main.&#34;SKLNK_RCDZKG&#34;&#62; &#60;&#62; 0 then         //---якщо породжено із замовлення&#13;&#10;      begin&#13;&#10;      selectDogOutNumFromPrdZkg.ParamByName('SKLNKRCD').Value:=&#60;main.&#34;SKLNK_RCD&#34;&#62;;&#13;&#10;      selectDogOutNumFromPrdZkg.Open;&#13;&#10;      osnova := 'Договір № ' + &#60;selectDogOutNumFromPrdZkg.&#34;Dog_OutNum&#34;&#62; + ' від ' + datetostr(&#60;selectDogOutNumFromPrdZkg.&#34;Dog_DtDog&#34;&#62;);&#13;&#10;   end;&#13;&#10;   if &#60;main.&#34;SKLNK_RCDZKG&#34;&#62; = 0 then&#13;&#10;      begin&#13;&#10;      if &#60;main.&#34;SKLNK_RCDDOG&#34;&#62; &#60;&#62; 0 then                                             //---якщо породжено із договору&#13;&#10;         begin&#13;&#10;         selectDogOutNumFromSklnk.ParamByName('SKLNKRCD').Value:=&#60;main.&#34;SKLNK_RCD&#34;&#62;;&#13;&#10;         selectDogOutNumFromSklnk.Open;&#13;&#10;         osnova := 'Договір № ' + &#60;selectDogOutNumFromSklnk.&#34;Dog_OutNum&#34;&#62; + ' від ' + datetostr(&#60;selectDogOutNumFromSklnk.&#34;Dog_DtDog&#34;&#62;);&#13;&#10;      end;&#13;&#10;      if &#60;main.&#34;SKLNK_RCDDOG&#34;&#62; = 0 then&#13;&#10;         begin&#13;&#10;         if &#60;main.&#34;SKLNK_RCDACC&#34;&#62; &#60;&#62; 0 then                             //---якщо породжено із рахунку&#13;&#10;            begin&#13;&#10;            selectDogOutNumFromAcc.ParamByName('SKLNKRCD').Value:=&#60;main.&#34;SKLNK_RCD&#34;&#62;;&#13;&#10;            selectDogOutNumFromAcc.Open;&#13;&#10;            if &#60;selectDogOutNumFromAcc.&#34;Dog_OutNum&#34;&#62; &#60;&#62; '' then        //---якщо в рахунку зазначено документ-підставу &#34;Договір&#34;&#13;&#10;               begin&#13;&#10;               osnova := 'Договір № ' + &#60;selectDogOutNumFromAcc.&#34;Dog_OutNum&#34;&#62; + ' від ' + datetostr(&#60;selectDogOutNumFromAcc.&#34;Dog_DtDog&#34;&#62;);&#13;&#10;            end;&#13;&#10;            if &#60;selectDogOutNumFromAcc.&#34;Dog_OutNum&#34;&#62; = '' then         //---якщо в рахунку не зазначено документ-підставу &#34;Договір&#34;&#13;&#10;               begin&#13;&#10;               if ((&#60;Main.&#34;SklNk_OsnCd&#34;&#62; =0) OR (&#60;Main.&#34;SklNk_OsnCd&#34;&#62; =3)) then&#13;&#10;                  osnova := iif(&#60;main.&#34;Osn&#34;&#62; = 'Счет', &#60;main.&#34;Osn&#34;&#62;, 'Рахунок') + ' '+ &#60;main.&#34;OsnNmr&#34;&#62;&#13;&#10;               else if(ValidDate(&#60;main.&#34;OsnDat&#34;&#62;)) then&#13;&#10;                  osnova := iif(&#60;main.&#34;Osn&#34;&#62; = 'Счет', &#60;main.&#34;Osn&#34;&#62;, 'Рахунок') + ' '+ &#60;main.&#34;OsnNmr&#34;&#62; + ' від ' + BPRO.FDATE_STR(&#60;main.&#34;OsnDat&#34;&#62;, 2) +'р.'&#13;&#10;               else&#13;&#10;                  osnova := iif(&#60;main.&#34;Osn&#34;&#62; = 'Счет', &#60;main.&#34;Osn&#34;&#62;, 'Рахунок') + ' '+ &#60;main.&#34;OsnNmr&#34;&#62;;&#13;&#10;            end;&#13;&#10;         end;&#13;&#10;      end;&#13;&#10;   end;&#13;&#10;   selectDogOutNumFromPrdZkg.Close;&#13;&#10;   selectDogOutNumFromAcc.Close;&#13;&#10;   selectDogOutNumFromSklnk.Close;&#13;&#10;end;&#13;&#10;//END---10.05.18---ВВ-Михайлюк А.Ю.---//---------------------------------------------------------------------------------------------------&#13;&#10;&#13;&#10;procedure Memo37OnBeforePrint(Sender: TfrxComponent);  //--artur - ставки пдв&#13;&#10;begin&#13;&#10;&#13;&#10;if (&#60;MAIN.&#34;SKLNK_TAXMDRCD&#34;&#62;) = 1 then&#13;&#10;begin&#13;&#10;taxmd:= 'ПДВ 20%';&#13;&#10;end;&#13;&#10;&#13;&#10;if (&#60;MAIN.&#34;SKLNK_TAXMDRCD&#34;&#62;) = 2 then&#13;&#10;begin&#13;&#10;taxmd:= 'ПДВ 7%';&#13;&#10;end;&#13;&#10;&#13;&#10;if (&#60;MAIN.&#34;SKLNK_TAXMDRCD&#34;&#62;) = 5 then&#13;&#10;begin&#13;&#10;taxmd:= 'ПДВ 0%';&#13;&#10;end;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnCloseQuery(Sender: TfrxComponent; var CanClose: Boolean);&#13;&#10;begin&#13;&#10;  if Page1.ModalResult=2 then StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;&#13;&#10;end.">
