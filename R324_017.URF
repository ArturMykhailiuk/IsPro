    <TfrxODBCQuery Name="main" UserName="main" CloseDataSource="true" FieldAliases.Text="" BCDToCurrency="false" IgnoreDupParams="false" SQL.Text="SELECT &#13;&#10; CRTFRM_NM,CRTFRM_LNNM,CrtFrm_OKPO,CrtSch_Sch,Bank_nm,Bank_MFO,CRTFRM_OtcTel&#13;&#10;,CRTADR_IND, sAdrCnt_Nm as CRTADR_CNTNM, CRTADR_REGNM, CRTADR_RAINM, CRTADR_NASNM, CRTADR_TOWNM, CRTADR_STRNM, CRTADR_HOUSE, CRTADR_NUMKOR, CRTADR_NUMKV -- ADDRESS                        &#13;&#10;,SKLNK_RCD,  SKLNK_RCDZKG, SKLNK_RCDDOG, SKLNK_RCDACC&#13;&#10;,SKLNK_OSNCD, SKLNK_CMT&#13;&#10;,SKLNK_KAGIDNM&#13;&#10;,SklNk_GplRcd&#13;&#10;,SklNk_KAgRcd                      &#13;&#10;,(select Filia_adr from PtnFilk KAg where KAg.Ptn_rcd = SklNk_KAgRcd and KAg.Filia_rcd = SklNk_KagAdr) KAg_adr&#13;&#10;,(select Ptn_SchRch from ptnRk where Ptn_rcd=SklNk_KAgRcd) KAg_sch&#13;&#10;,(select Filia_adr from PtnFilk where Ptn_rcd = SklNk_GplRcd and Filia_rcd = SklNk_GplAdr) Gpl_adr&#13;&#10;,(select Ptn_SchRch from ptnRk where Ptn_rcd=SklNk_GplRcd) Gpl_sch                &#13;&#10;,(select Ptn_nmsh from PtnRk where Ptn_rcd=SklNk_GplRcd) GplNm&#13;&#10;,case SklNk_OsnCd&#13;&#10;when 0 then 'Отсутствует'  &#13;&#10;when 1 then 'Договір №'  &#13;&#10;when 2 then 'Счет  № '  &#13;&#10;when 3 then 'Заказ  № '  &#13;&#10;when 4 then 'Заказ  № '&#13;&#10;end Osn&#13;&#10;  &#13;&#10;,case SklNk_OsnCd&#13;&#10;when 0 then ''  &#13;&#10;when 1 then Dog.Dog_Num                                         &#13;&#10;when 2 then Acc.Acc_NmrExt        &#13;&#10;when 3 then SklNk_NmrExt  &#13;&#10;when 4 then PrdZkg.PrdZkg_Nmr&#13;&#10;end OsnNmr&#13;&#10;&#13;&#10;,case SklNk_OsnCd&#13;&#10;when 0 then SKLNK_DAT              &#13;&#10;when 1 then Dog.Dog_DtDog                                             &#13;&#10;when 2 then Acc.Acc_Dat      &#13;&#10;when 3 then SKLNK_DAT  &#13;&#10;when 4 then PrdZkg.PrdZkg_Dt  &#13;&#10;end OsnDat      &#13;&#10;&#13;&#10;,SKLNK_NMR&#13;&#10;,SKLNK_DAT&#13;&#10;,SKLNK_TAXMDRCD&#13;&#10;,SKLNK_NMREXT                            &#13;&#10;,TRDSPOS.SKLN_CD&#13;&#10;,TRDSPOS.SKLN_NM&#13;&#10;,EI_SHNM&#13;&#10;,TRDSPOS.TRDSTYPE        &#13;&#10;,TRDSPOS.QT&#13;&#10;,TRDSPOS.CN&#13;&#10;,TRDSPOS.SUMBEZNAL&#13;&#10;,TRDSPOS.NDSSUM&#13;&#10;,TRDSPOS.RCD&#13;&#10;,TRDSPOS.Skln_Rcd&#13;&#10;,SVL_CD, SVL_RCD VALRCD&#13;&#10;,SKLNK_TDOVFIO                                &#13;&#10;,SKLNK_TDOVNMR                                                     &#13;&#10;,SKLNK_TDOVDAT , /*t.Aktivn, t.Altrnzv, t.Kod_Dk, t.Kod_OON,*/ TRDSPOS.TrdS_TarFlg&#13;&#10;,SklUd_Nm      &#13;&#10;--, t1.Aktivn_SPR, t1.Kod_Dk_SPR, t1.Kod_OON_SPR&#13;&#10;,User_FIO                      &#13;&#10;FROM SKLNK&#13;&#10;inner join&#13;&#10;(&#13;&#10;       select &#13;&#10;       TrdS_Rcd as RCD&#13;&#10;       ,TrdS_RcdHdr&#13;&#10;       ,Trds_Type as TRDSTYPE             &#13;&#10;       ,TrdS_TarFlg,Skln_rcd,SKLN_CD&#13;&#10;       ,SKLN_NM&#13;&#10;       ,TRDS_EIQT as EIQT&#13;&#10;       ,TRDS_QTCN as QT --KOLICHESTVO&#13;&#10;       --,TRDS_CN as CN --CENA BEZ NALOGOV&#13;&#10;       --,TRDS_SUM as SUMBEZNAL -- SUMMA BEZ NALOGOV&#13;&#10;       --,case when ATT1.TRDSTAX_SUM is not null then ATT1.TRDSTAX_SUM else 0 end as NDSSUM&#13;&#10;       --,TrdS_cnskl CENASNAL                 &#13;&#10;       ,TRDS_SUMTax + case when b1.nds is null then 0 else b1.nds END NDSSUM        &#13;&#10;       ,case when TrdSTax_Flg&#38;0x04=0 then TrdS_cn else case when TRDS_QT=0 then 0 else TrdS_cn-TrdS_SumTax/TRDS_QT end end CN&#13;&#10;       ,TRDS_SUMOpl-TrdS_SumTax SUMBEZNAL      &#13;&#10;           &#13;&#10;       from TRDS&#13;&#10;       inner Join SklN on TrdS.TrdS_RcdNom = SklN.SklN_Rcd&#13;&#10;       left join ATRDSTAX on TrdS_Rcd = ATrdSTax_RcdS and TrdSTax_Cd = 1 AND ATrdSTax_RcdAs =0  &#13;&#10;       --left join (ATRDSTAX ATT1 left join TAXRATE TR1 on TR1.TaxRate_Rcd = ATT1.TrdSTax_RateCd) on TrdS_Rcd = ATT1.ATrdSTax_RcdS and ATT1.ATrdSTax_RcdAs = 0 and ATT1.TrdSTax_Cd = 1&#13;&#10;       left join TAXRATE on TrdSTax_RateCd = TaxRate_Rcd&#13;&#10;       left join (&#13;&#10;  Select TrdS_Rcd rcd, &#13;&#10;  SUM(case when at.TrdSTax_Flg&#38;0x04=0 then at.TrdSTax_SumBas else at.TrdSTax_SumBas - at.TrdSTax_Sum END) smBezNds,  &#13;&#10;  SUM(case when at.TrdSTax_Flg&#38;0x04=0 then a.TrdS_SumOpl-a.TrdS_Sum else at.TrdSTax_Sum END) nds      &#13;&#10;  from trds  t&#13;&#10;  inner join atrds a on t.TrdS_Rcd=a.ATrds_RcdS&#13;&#10;  inner join ATRDSTAX at on t.TrdS_Rcd = at.ATrdSTax_RcdS AND ATrdSTax_RcdAs &#62;0&#13;&#10;  group by TrdS_Rcd&#13;&#10;) b1 on b1.rcd = TrdS.TrdS_Rcd         &#13;&#10;       where TrdS_TypHdr = 1 and TrdS_Type &#60;&#62; 5&#13;&#10;&#13;&#10;         &#13;&#10;       union all&#13;&#10;       select&#13;&#10;       ATrdS_Rcd as RCD&#13;&#10;       ,TrdS_RcdHdr                           &#13;&#10;       ,ATR.TrdS_Type as TRDSTYPE           &#13;&#10;       ,TrdS_TarFlg,Skln_rcd,SKLN_CD&#13;&#10;       ,SKLN_NM&#13;&#10;       ,ATR.TRDS_EIQT as EIQT&#13;&#10;       ,ATR.TRDS_QTCN as QT --KOLICHESTVO&#13;&#10;       ,ATR.TRDS_CN as CN --CENA BEZ NALOGOV&#13;&#10;       ,ATR.TRDS_SUM as SUMBEZNAL -- SUMMA BEZ NALOGOV&#13;&#10;       ,case when ATT1.TRDSTAX_SUM is not null then ATT1.TRDSTAX_SUM else 0 end as NDSSUM&#13;&#10;          &#13;&#10;       from TRDS&#13;&#10;       inner join ATRDS ATR on ATrdS_RcdS = TrdS_Rcd&#13;&#10;       inner Join SklN ON SklN_Rcd = ATR.TrdS_RcdNom&#13;&#10;       left join ATRDSTAX on TrdS_Rcd = ATrdSTax_RcdS and TrdSTax_Cd = 1 AND ATrdSTax_RcdAs =0  &#13;&#10;       left join (ATRDSTAX ATT1 left join TAXRATE TR1 on TR1.TaxRate_Rcd = ATT1.TrdSTax_RateCd) on TrdS_Rcd = ATT1.ATrdSTax_RcdS and ATrdS_Rcd = ATT1.ATrdSTax_RcdAs and ATT1.TrdSTax_Cd = 1&#13;&#10;  &#13;&#10;       where TrdS_TypHdr = 1&#13;&#10;) TRDSPOS on TRDSPOS.TrdS_RcdHdr = SklNk_Rcd&#13;&#10;----=-==-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=--&#13;&#10;--left join (select NmChrct_DocRcd, NmChrct_PosRcd ,&#13;&#10;  --                max(case when nmchrct_Num=1 then NmChrct_val else '' end ) Aktivn,&#13;&#10;    --              max(case when nmchrct_Num=5 then NmChrct_val else '' end ) Altrnzv,&#13;&#10;      --            max(case when nmchrct_Num=4 then NmChrct_val else '' end ) Kod_Dk,                    &#13;&#10;        --          max(case when nmchrct_Num=3 then NmChrct_val else '' end ) Kod_OON&#13;&#10;          -- from  Nmchrct&#13;&#10;            --where nmchrct_Num in (1,5,4,3) and NmChrct_ChrtTp = 0 and NmChrct_TypHdr =	1   &#13;&#10;              --group by NmChrct_DocRcd, NmChrct_PosRcd                                                &#13;&#10;                --) t on NmChrct_DocRcd=SklNk_Rcd and  NmChrct_PosRcd =TRDSPOS.RCD&#13;&#10;----=-==-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=--t альтернативные названия из расх накладных&#13;&#10;--left join (select NmChrct_RcdNom,&#13;&#10;  --                max(case when nmchrct_cd=1 then NmChrct_val else '' end ) Aktivn_SPR,&#13;&#10;    --              max(case when nmchrct_cd=5 then NmChrct_val else '' end ) Altrnzv,&#13;&#10;      --            max(case when nmchrct_cd=4 then NmChrct_val else '' end ) Kod_Dk_SPR,&#13;&#10;        --          max(case when nmchrct_cd=3 then NmChrct_val else '' end ) Kod_OON_SPR            &#13;&#10;          -- from  Nmchrct&#13;&#10;            -- where nmchrct_cd in (1,5,4,3) and NmChrct_TypHdr = '35'&#13;&#10;		            -- group by NmChrct_RcdNom) t1 on t1.NmChrct_RcdNom=SKLN_Rcd&#13;&#10;----=-==-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=--  t1 это из справочника, используем отсюда коды ДК ООН и активность, название тянем непостредственно из справочника                                                             &#13;&#10;left join EI on EI_Rcd = TRDSPOS.EIQT&#13;&#10;left join SVL on SVl_Rcd = SklNk_ValCd&#13;&#10;left join CRTFRM1 on CRTFRM_Nm&#60;&#62;' '&#13;&#10;left join CRTADR on CRTADR_cd=1 &#13;&#10;left join CrtSch on SklNk_SlfSch = CrtSch_rcd&#13;&#10;left join bank on CrtSch_Bank = Bank_rcd&#13;&#10;left join sAdrUCnt on CRTADR_CntCD = sAdrCnt_Cd    &#13;&#10;LEFT JOIN ACC ON Acc.Acc_Rcd=SKLNK.SklNk_RcdAcc&#13;&#10;LEFT JOIN Dog ON dog.Dog_Rcd=SklNk_RcdDog &#13;&#10;LEFT JOIN PrdZkg ON PrdZkg.PrdZkg_Rcd=SklNk_RcdZkg&#13;&#10;LEFT JOIN SklUd on SklUd_Cd = PrdZkg_UslD&#13;&#10;LEFT JOIN /*TABLE*/ User ON User_Rcd = Sklnk_CdUsr                                                                                                                                                                                                                                             " PreprocessSQLText="true" SQLZeroCols="false" PropData="05704C65667402280470546F7002240A506172616D65746572730100"/>
