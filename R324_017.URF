    <TfrxODBCQuery Name="selectNmChrct" UserName="selectNmChrct" CloseDataSource="true" FieldAliases.Text="" BCDToCurrency="false" IgnoreDupParams="false" SQL.Text="with Chr_SPR (SprNum, SprNm, SprVal) &#13;&#10;AS&#13;&#10;(SELECT NmChrct_Num SprNum, Nmchrct_Nm SprNm, NmChrct_Val SprVal FROM NmChrct WHERE NmChrct_TypHdr = 35 AND NmChrct_RcdNom = :SklnRcd AND NMCHRCT_DocRcd = :SklnRcd),&#13;&#10;Chr_SKLNK (SklnkNum, SklnkNm, SklnkVal) &#13;&#10;AS&#13;&#10;(SELECT NmChrct_Num SklnkNum, Nmchrct_Nm SklnkNm, Nmchrct_Val SklnkVal FROM NmChrct WHERE NmChrct_TypHdr = 1 AND NmChrct_RcdNom = :SklnRcd AND NMCHRCT_DocRcd = :SklnkRcd)&#13;&#10;SELECT SprNum, SprNm, SprVal, SklnkNum, SklnkNm, SklnkVal FROM chr_SPR INNER JOIN chr_SKLNK ON Chr_SPR.SprNum = Chr_SKLNK.SklnkNum&#13;&#10;WHERE SprNm = :ChrctName                                                       " PreprocessSQLText="true" SQLZeroCols="true" PropData=""/>
